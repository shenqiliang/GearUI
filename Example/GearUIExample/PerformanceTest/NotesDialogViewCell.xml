<TableViewCell class="NotesDialogViewCell">
    <LayoutView top="0" left="10" right="10" bottom="15" backgroundColor="#41485A 100%" cornerRadius="3" clipsToBounds="1">
        <LayoutView id="title" left="0" right="0" height="36" top="0" backgroundColor="#3A4152 100%">
            <ImageView left="12" top="8" width="20" height="20">ico_note</ImageView>
            <Button id="feedback" right="12" top="8" width="20" height="20" image="反馈" touchEdgeInsets="{-20, -20, -20, -20}"></Button>
        </LayoutView>
        
        <LayoutView id="chat" top="title.bottom" left="0" right="0">
            <Label id="text" left="12" top="18" font="15" textColor="#FBFCFE 100%" numberOfLines="0" lineSpace="9" right="12">查看我的备忘</Label>
            <View id="sep" left="12" right="12" top="text.bottom+18" bottom="0" height="0.5" backgroundColor="#FFFFFF 10%"/>
        </LayoutView>
        
        <Label id="count" left="12" top="chat.bottom+18" font="17" right="12" numberOfLines="0" textColor="#FBFCFE 100%" disableLayout="true">好的，你有9个备忘</Label>
        
        <LayoutView id="notes1" top="count.bottom+6|chat.bottom+6" height="57" left="0" right="0">
            <Label id="content" left="12" centerY="super.centerY" font="17" textColor="#FBFCFE 100%" numberOfLines="0">手机号码123456789</Label>
        </LayoutView>
        
        <LayoutView id="notes2" top="notes1.bottom" height="57" left="0" right="0" disableLayout="true" >
            <Label id="content" left="12" centerY="super.centerY" font="17" textColor="#FBFCFE 100%" numberOfLines="0">手机号码123456789</Label>
        </LayoutView>
        
        <LayoutView id="notes3" top="notes2.bottom" height="57" left="0" right="0"  disableLayout="true">
            <Label id="content" left="12" centerY="super.centerY" font="17" textColor="#FBFCFE 100%" numberOfLines="0">手机号码123456789</Label>
        </LayoutView>
        
        <LayoutView left="0" right="0" top="notes3.bottom|notes2.bottom|notes1.bottom" height="44" bottom="0">
            <Button id="detail_button" right="12" centerY="super.centerY" width="62" height="24" image="Setting_btn_next" layoutType="2" titleColor="#7A7E89 100%" font="13">查看全部</Button>
        </LayoutView>
    </LayoutView>
</TableViewCell>
