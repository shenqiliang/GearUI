<ViewController title="" backgroundColor="#2D323F 100%">
    <TableView left="0" right="0" top="0" bottom="0" id="#tableView" backgroundColor="#2D323F 100%" separatorStyle="none" contentInset="{0,0,10,0}">
        <TableViewCell id="banner_cell" class="DiscoverTableViewBannerCell" backgroundColor="#2D323F 100%" selectionStyle="none">
