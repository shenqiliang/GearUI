            <PageControl id="page" left="banner.left" right="banner.right" bottom="banner.bottom" height="20"/>
        </TableViewCell>
        
        <TableViewCell id="features_cell" class="DiscoverTableViewFeaturesCell" backgroundColor="clear" selectionStyle="none">
            <LayoutView id="layout_0" left="5" top="0" width="super.width*0.2-2" bottom="0" backgroundColor="clear">
                <ImageView id="item" top="15" bottom="27.5" centerX="super.centerX" width="super.width*0.66" height="item.width" clipsToBounds="1" backgroundColor="clear" contentMode="fill" highlightedCoverColor="#000000 50%" hidden="1">Music_img_cover</ImageView>
                <Label id="name" left="super.left" top="item.bottom+9" font="13" right="super.right" textColor="white" textAlignment="center" >音乐</Label>
                <Button id="button" tap="miapage://profile/services" delaysHighlighting="yes" centerX="item.centerX" centerY="item.centerY" width="item.width" height="item.width" backgroundImage="clear" touchEdgeInsets="{-20, -20, -20, -20}" />
            </LayoutView>
            
            <LayoutView id="layout_1" left="layout_0.right" top="0" width="super.width*0.2-2" height="layout_0.height">
                <ImageView id="item" top="15" centerX="super.centerX" width="super.width*0.66" height="item.width" clipsToBounds="1" backgroundColor="clear" contentMode="fill" highlightedCoverColor="#000000 50%" hidden="1">Music_img_cover</ImageView>
                <Label id="name" left="super.left" top="item.bottom+9" font="13" right="super.right" textColor="white" textAlignment="center" >儿童故事</Label>
                <Button id="button" tap="miapage://profile/services" delaysHighlighting="yes" centerX="item.centerX" centerY="item.centerY" width="item.width" height="item.width" backgroundImage="clear" touchEdgeInsets="{-20, -20, -20, -20}" />
            </LayoutView>
            
            <LayoutView id="layout_2" left="layout_1.right" top="0" width="super.width*0.2-2" height="layout_0.height">
                <ImageView id="item" top="15" centerX="super.centerX" width="super.width*0.66" height="item.width" clipsToBounds="1" backgroundColor="clear" contentMode="fill" highlightedCoverColor="#000000 50%" hidden="1">Music_img_cover</ImageView>
                <Label id="name" left="super.left" top="item.bottom+9" font="13" right="super.right" textColor="white" textAlignment="center" >有声书</Label>
                <Button id="button" tap="miapage://profile/services" delaysHighlighting="yes" centerX="item.centerX" centerY="item.centerY" width="item.width" height="item.width" backgroundImage="clear" touchEdgeInsets="{-20, -20, -20, -20}" />
            </LayoutView>
            
            <LayoutView id="layout_3" left="layout_2.right" top="0" width="super.width*0.2-2" height="layout_0.height">
                <ImageView id="item" top="15" centerX="super.centerX" width="super.width*0.66" height="item.width" clipsToBounds="1" backgroundColor="clear" contentMode="fill" highlightedCoverColor="#000000 50%" hidden="1">Music_img_cover</ImageView>
                <Label id="name" left="super.left" top="item.bottom+9" font="13" right="super.right" textColor="white" textAlignment="center" >生活助手</Label>
                <Button id="button" tap="miapage://profile/services" delaysHighlighting="yes" centerX="item.centerX" centerY="item.centerY" width="item.width" height="item.width" backgroundImage="clear" touchEdgeInsets="{-20, -20, -20, -20}" />
            </LayoutView>
        
            <LayoutView id="layout_4" left="layout_3.right" top="0" width="super.width*0.2-2" height="layout_0.height">
                <ImageView id="item" top="15" centerX="super.centerX" width="super.width*0.66" height="item.width" clipsToBounds="1" backgroundColor="clear" contentMode="fill" highlightedCoverColor="#000000 50%" hidden="1">Music_img_cover</ImageView>
                <Label id="name" left="super.left" top="item.bottom+9" font="13" right="super.right" textColor="white" textAlignment="center" >技能</Label>
